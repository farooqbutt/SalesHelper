    <!-- Conditional loading of scripts -->
    <script>
        // Get the current URL
        var currentUrl = window.location.pathname;

        // Check if the current URL matches the specific URL
        if (currentUrl === "/" || currentUrl == "/Home/Index") {
            var script = document.createElement('script');
            script.src = '/js/scripts/pages/dashboard-ecommerce.js';
            document.head.appendChild(script);
        } else if (currentUrl === "/Home/AnalyticsView") {
            var script = document.createElement('script');
            script.src = '/js/scripts/pages/dashboard-analytics.js';
            document.head.appendChild(script);
        }
    </script>

    <script>
        // Function to set the active item and store it in localStorage
        function setActiveItem(itemIndex) {
            localStorage.setItem('activeItem', itemIndex);
        }

        function setActiveNavItem(itemIndex) {
            localStorage.setItem('activeNavItem', itemIndex);
        }

        function setActiveNavItemWithoutSubItem(itemIndex) {
            localStorage.setItem('activeNavItemWithoutSubItem', itemIndex);
        }

        // Function to initialize the active item based on localStorage
        function initializeActiveItem() {
            const activeItemIndex = localStorage.getItem('activeItem');
            const activeNavItem = localStorage.getItem('activeNavItem');
            const activeNavItemWithoutSubItem = localStorage.getItem('activeNavItemWithoutSubItem');

            if (activeItemIndex !== null) {
                // Remove "active" class from all <li> elements
                document.querySelectorAll('li').forEach(li => {
                    li.classList.remove('active');
                });

                // Add "active" class to the <li> element based on the stored index
                const menuItems = document.querySelectorAll('.menu-item');
                if (menuItems[activeItemIndex]) {
                    menuItems[activeItemIndex].closest('li').classList.add('active');
                    localStorage.removeItem('activeNavItemWithoutSubItem');
                }
            }

            if (activeNavItem !== null) {
                // Remove "open" class from all <li> elements
                document.querySelectorAll('li[class="nav-item has-sub"]').forEach(li => {
                    li.classList.remove('open');
                });

                // Add "open" class to the <li> element based on the stored index
                const navItems = document.querySelectorAll('li[class="nav-item has-sub"]');
                if (navItems[activeNavItem]) {
                    navItems[activeNavItem].classList.add('open');
                    localStorage.removeItem('activeNavItemWithoutSubItem');
                }
            }

            if (activeNavItemWithoutSubItem !== null) {

                document.querySelector('ul[class="navigation navigation-main"]')
                    .querySelectorAll('li.nav-item').forEach(li => {
                        li.classList.remove('active');
                    });

                const navItemsWithoutHasSub = document.querySelector('ul[class="navigation navigation-main"]')
                    .querySelectorAll('li.nav-item');
                if (navItemsWithoutHasSub[activeNavItemWithoutSubItem]) {
                    navItemsWithoutHasSub[activeNavItemWithoutSubItem].classList.add('active');
                }
            }
        }

        // Add click event listeners to the menu items
        const menuItems = document.querySelectorAll('.menu-item');
        menuItems.forEach((item, index) => {
            item.addEventListener('click', () => {
                setActiveItem(index);
            });
        });

        // Add click event listeners to the nav items
        const navItems = document.querySelectorAll('li[class="nav-item has-sub"]');
        navItems.forEach((item, index) => {
            item.addEventListener('click', () => {
                setActiveNavItem(index);
            });
        });

        // Add click event listeners to the nav items
        const navItems_without_has_sub = document.querySelector('ul[class="navigation navigation-main"]')
            .querySelectorAll('li.nav-item');
        navItems_without_has_sub.forEach((item, index) => {
            item.addEventListener('click', () => {
                setActiveNavItemWithoutSubItem(index);
            });
        });

        // Initialize the active item when the page loads
        window.addEventListener('load', initializeActiveItem);
    </script>

